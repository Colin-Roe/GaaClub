@model GaaClub.Models.Member

@{
    ViewData["Title"] = "Import";
}

<h1>Import Members</h1>

<form method="post" enctype="multipart/form-data" asp-controller="Members" asp-action="UploadMembers">
    <div class="form-group">
        <div class="col-md-10">
            <p>Upload members using this form:</p>
            <input id="uploadMembers" type="file" name="files" multiple />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>

